{
  "name": "door43-preview-renderers",
  "version": "1.4.0",
  "description": "JavaScript library for gathering content from various repositories and rendering HTML snippets for web previews and PDFs",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "bin": {
    "door43-renderers": "./src/cli.js"
  },
  "files": [
    "dist",
    "src/cli.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cli": "node src/cli.js",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "styleguide": "styleguidist server",
    "styleguide:dev": "styleguidist server",
    "styleguide:build": "pnpm run build && styleguidist build",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "door43",
    "preview",
    "renderer",
    "html",
    "pdf",
    "bible",
    "translation"
  ],
  "author": "unfoldingWord",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unfoldingWord/door43-preview-renderers.git"
  },
  "bugs": {
    "url": "https://github.com/unfoldingWord/door43-preview-renderers/issues"
  },
  "homepage": "https://github.com/unfoldingWord/door43-preview-renderers#readme",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "netlify-cli": "^23.11.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-styleguidist": "^13.1.2",
    "rollup": "^4.1.4",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "js-yaml": "^4.1.1",
    "jszip": "^3.10.1",
    "usfm-js": "^3.4.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "pnpm": {
    "overrides": {}
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  }
}
